// Write a program to find the sum of the squares of the first 9 natural numbers that are divisible by 3.

let  sum = 0;
let sqr = 0;
let i = 0;
let n = 1;
while(i < 9){
    if (n % 3 === 0){
        let sqr = n * n ;
        sum = sum + sqr ;
        i = i +1 ;
    }
    n = n + 1 ;
}
console.log(sum);
