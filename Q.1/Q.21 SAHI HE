let a = 12, b = 18, c = 24, d = 30;
let x = a, y = b;

while (y !== 0) {
  let t = y;
  y = x % y;
  x = t;
}
let hcf = x;

x = hcf; y = c;
while (y !== 0) {
  let t = y;
  y = x % y;
  x = t;
}
hcf = x;

x = hcf; y = d;
while (y !== 0) {
  let t = y;
  y = x % y;
  x = t;
}
hcf = x;

console.log(hcf);
