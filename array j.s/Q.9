// Write a program to find the sum and average of elements in an array. Take elements as input from the user.

let arr = [5,10,20,30,40,25];
let sum = 0;
let n = 6;


for (let i = 0; i < n; i++) {
    
    sum = sum + arr[i];
}


let average = sum / n;


console.log("Sum =", sum,"Average =", average);
