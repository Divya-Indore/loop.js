 // Write a program to print duplicates in an array. Create the array by taking the array size and elements from the user.
 
let arr = [4,3,5,4,6,7];
let size = 6;
let duplicates = "";
for (let i = 0; i < 6; i++){
    for(let j = i + 1; j < 6; j++){
        if(arr [i] === arr[j]){
            duplicates = duplicates + arr[i] + "";
            break;
        }
    }
}
console.log("Duplicates :", duplicates);
