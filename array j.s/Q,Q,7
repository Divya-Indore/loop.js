// Write a program to find the sum of all diagonal elements of a matrix.

FOR LOOP :-

let matrix = [
[1, 2, 3],
  [4, 5, 6],
  [7, 8, 9]
];

let sum = 0;


for (let i = 0; i < matrix.length; i++) {
  sum = sum + matrix[i][i]; 
}

console.log("Sum of diagonal elements:", sum);

WHILE LOOP :-

let matrix = [
  [1, 2, 3],
  [4, 5, 6],
  [7, 8, 9]
];



let sum = 0;
let i = 0;

while (i < matrix.length) {
  sum = sum + matrix[i][i];   
  i++;
}

console.log("Sum of diagonal elements:", sum);




